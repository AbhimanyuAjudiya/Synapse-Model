    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "ioredis": "^5.3.2",
    "mongoose": "^8.1.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "redis": "^4.6.12",
    "uuid": "^9.0.1"
